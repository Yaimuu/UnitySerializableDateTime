#day-cell {
    flex-grow: 1;
    -unity-font-style: bold;
    font-size: 14px;
    -unity-text-align: middle-center;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
}

#titleLabel {
    flex-grow: 1;
    -unity-text-align: middle-center;
    font-size: 20px;
}

.selected {
    background-color: rgb(37, 99, 235);
}

.disabled {
    color: rgb(67, 67, 67);
}
